{
  "name": "brew-planner",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "pnpm run build-server && pnpm run build-client",
    "build-client": "vite build",
    "check-and-build-client": "tsc --noEmit && vite build",
    "build-server": "cd api\ntsc --outDir ../build/api --target es2015 --moduleResolution node --allowSyntheticDefaultImports true --module esnext\ncd ..",
    "client": "vite",
    "dev": "concurrently \"pnpm run server\" \"pnpm run client\"",
    "preview": "vite preview",
    "prodtest": "PROD_MODE=true\nexport PROD_MODE\npnpm run build\nnode build/server/server.js",
    "create-docs": "apidoc -i api/src/ -o doc/",
    "server": "TEST_MODE=true\nexport TEST_MODE\napidoc -i api/src/ -o doc/\nnode api/src/server/app.js",
    "start": "PROD_MODE=true\nexport PROD_MODE\npnpm run build\nforever start -o console_log.txt -e console_err.txt ./build/server/server.js",
    "stop": "forever stop build/server/server.js\n./save_log.sh",
    "restart": "forever stop build/server/server.js\n./save_log.sh\nPROD_MODE=true\nexport PROD_MODE\npnpm run build\nforever start -o console_log.txt -e console_err.txt ./build/server/server.js",
    "test": "mocha --recursive './api/src/**/*test.js' './api/test/*test.js' -grep $1",
    "lint": "npx eslint -c .eslintrc.cjs '**/*'",
    "unit-test": "mocha --recursive './api/src/**/*test.js' -g $1",
    "int-test": "mocha --recursive './api/test/*test.js' -g $1"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@firebase/app-types": "^0.7.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "5.0.0-alpha.91",
    "@mui/material": "^5.8.6",
    "@mui/styled-engine-sc": "^5.8.0",
    "@tanstack/react-query": "^4.0.8",
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "firebase": "^9.8.4",
    "firebase-admin": "^11.0.1",
    "ip": "^1.1.8",
    "jspdf": "^2.5.1",
    "module": "^1.2.5",
    "morgan": "^1.10.0",
    "mui-datatables": "^4.2.2",
    "mysql2": "^2.3.3",
    "node-mysql-deadlock-retries": "^2.0.5",
    "qs": "^6.11.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.33.0",
    "react-is": ">= 16.8.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5",
    "uuid": "^8.3.2",
    "xss": "^1.0.13"
  },
  "devDependencies": {
    "@babel/core": ">=7.0.0 <8.0.0",
    "@mui/types": "^7.1.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/firebase": "^3.2.1",
    "@types/mui-datatables": "^3.7.9",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "apidoc": "^0.52.0",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "forever": "^4.0.3",
    "http-proxy-middleware": "^2.0.6",
    "inquirer": "^9.1.0",
    "lab": "link:@types/@mui/lab",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4",
    "vite": "^2.9.9",
    "vite-plugin-eslint": "^1.6.1"
  },
  "pnpm": {
    "overrides": {
      "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
    }
  }
}
